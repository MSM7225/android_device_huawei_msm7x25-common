#
# system.prop for Huawei MSM7x25 Board
#

# RIL properties
rild.libargs=-d /dev/smd0

# zRam properties
ro.zram.default=0

# Audio properties
persist.sys.fm-attn=0
persist.sys.headset-attn=0
persist.sys.speaker-attn=6

# SD card readahead buffer (MSM7x25Parts)
persist.sys.read_ahead_kb=0

# Background process limit (MSM7x25Parts)
persist.sys.processlimit=2
